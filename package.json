        "email": "commaniakr@gmail.com"
    },
    "repository": "https://github.com/hotheadfactory/BugsElectronPlayer",
    "license": "MIT",
    "devDependencies": {
        "asar": "^2.0.1",
        "electron": "^1.8.8",
        "electron-builder": "^13.9.0",
        "electron-packager": "^14.0.3",
        "electron-rebuild": "^1.5.7"
    },
    "build": {
        "appId": "OrangePlayer",
        "asar": false,
        "linux": {
            "target": [
                "dir"
