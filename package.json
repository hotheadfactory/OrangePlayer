{
    "name": "OrangePlayer",
    "productName": "OrangePlayer",
    "version": "1.3.1",
    "description": "OrangePlayer",
    "main": "./app/main.js",
    "scripts": {
        "start": "electron ./app/main.js",
        "build": "build -lm --x64",
        "dbus": "./node_modules/.bin/electron-rebuild -f -w dbus"
    },
    "author": "hotheadfactory",
    "bugs": {
